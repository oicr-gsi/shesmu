Input gsi_std;

# this a comment
Define standard_fastq(string s)
  Where workflow == { "BamQC 2.7+" ,3} [ 0 ] 
  Where metatype == "chemical/fastq-gzip"
  Where path==foo ( file_size )
  Group files = path By accession, donor;

#Comment 
Run fastqc
  Matches standard_fastq("a")#comment
  With {
    memory=4Gi ,#comment
    input = files
  }
