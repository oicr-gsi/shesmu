              "application/json": {
                "schema": {
                  "items": {
                    "properties": {
                      "description": {
                        "type": "string"
                      },
                      "name": {
                        "type": "string"
                      },
                      "type": {
                        "format": "typedescriptor",
                        "type": "string"
                      }
                    },
                    "type": "object"
                  },
                  "type": "array"
                }
              }
            },
            "description": "Constants catalog transmitted"
          }
        },
        "summary": "Get a list of constants that can be use in olives on this server.",
        "tags": [
          "constants"
        ]
      }
    },
