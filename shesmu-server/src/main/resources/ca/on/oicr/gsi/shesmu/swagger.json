{
  "components": {
    "schemas": {
      "Alert": {
        "properties": {
          "annotations": {
            "additionalProperties": {
              "type": "string"
            },
            "type": "object"
          },
          "endsAt": {
            "format": "date-time",
            "type": "string"
          },
          "generatorURL": {
            "format": "uri",
            "type": "string"
          },
          "id": {
            "type": "string"
          },
          "labels": {
            "additionalProperties": {
              "type": "string"
            },
            "type": "object"
          },
          "startsAt": {
            "format": "date-time",
            "type": "string"
          }
        },
        "type": "object"
      },
      "AnyValue": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "number"
          },
          {
            "type": "integer"
          },
          {
            "type": "boolean"
          },
          {
            "items": {},
            "type": "array"
          }
        ],
        "description": "Can be any value - string, number, boolean, array or object."
      },
      "CacheInfo": {
        "description": "Cache information",
        "discriminator": {
          "mapping": {
            "k": "#/components/schemas/CacheInfoKeyValue",
            "kv": "#/components/schemas/CacheInfoValue"
          },
          "propertyName": "type"
        },
        "oneOf": [
          {
            "$ref": "#/components/schemas/CacheInfoKeyValue"
          },
          {
            "$ref": "#/components/schemas/CacheInfoValue"
          }
        ]
      },
      "CacheInfoKeyValue": {
        "properties": {
          "entries": {
            "additionalProperties": {
              "properties": {
                "collectionSize": {
                  "description": "the number of items in this cache",
                  "type": "integer"
                },
                "lastUpdate": {
                  "description": "miliseconds since UNIX epoch",
                  "type": "integer"
                }
              },
              "type": "object"
            },
            "description": "the items stored in this cache",
            "type": "object"
          },
          "name": {
            "description": "the name of the cache",
            "type": "string"
          },
          "ttl": {
            "description": "the number of minutes before the cache is refreshed",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "CacheInfoValue": {
        "properties": {
          "collectionSize": {
            "description": "the number of items in this cache",
            "type": "integer"
          },
          "lastUpdate": {
            "description": "miliseconds since UNIX epoch",
            "type": "integer"
          },
          "name": {
            "description": "the name of the cache",
            "type": "string"
          },
          "ttl": {
            "description": "the number of minutes before the cache is refreshed",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "Filter": {
        "description": "Action filtering.",
        "discriminator": {
          "mapping": {
            "added": "#/components/schemas/FilterAdded",
            "addedago": "#/components/schemas/FilterAddedAgo",
            "and": "#/components/schemas/FilterAnd",
            "checked": "#/components/schemas/FilterChecked",
            "checkedago": "#/components/schemas/FilterCheckedAgo",
            "external": "#/components/schemas/FilterExternal",
            "externalago": "#/components/schemas/FilterExternalAgo",
            "id": "#/components/schemas/FilterId",
            "or": "#/components/schemas/FilterOr",
            "regex": "#/components/schemas/FilterRegex",
            "sourcefile": "#/components/schemas/FilterSourceFile",
            "sourcelocation": "#/components/schemas/FilterSourceLocation",
            "status": "#/components/schemas/FilterStatus",
            "statuschanged": "#/components/schemas/FilterStatusChanged",
            "statuschangedago": "#/components/schemas/FilterStatusChangedAgo",
            "tag": "#/components/schemas/FilterTag",
            "text": "#/components/schemas/FilterText",
            "type": "#/components/schemas/FilterType"
          },
          "propertyName": "type"
        },
        "oneOf": [
          {
            "$ref": "#/components/schemas/FilterAdded"
          },
          {
            "$ref": "#/components/schemas/FilterAddedAgo"
          },
          {
            "$ref": "#/components/schemas/FilterAnd"
          },
          {
            "$ref": "#/components/schemas/FilterChecked"
          },
          {
            "$ref": "#/components/schemas/FilterCheckedAgo"
          },
          {
            "$ref": "#/components/schemas/FilterExternal"
          },
          {
            "$ref": "#/components/schemas/FilterExternalAgo"
          },
          {
            "$ref": "#/components/schemas/FilterId"
          },
          {
            "$ref": "#/components/schemas/FilterOr"
          },
          {
            "$ref": "#/components/schemas/FilterRegex"
          },
          {
            "$ref": "#/components/schemas/FilterSourceFile"
          },
          {
            "$ref": "#/components/schemas/FilterSourceLocation"
          },
          {
            "$ref": "#/components/schemas/FilterStatus"
          },
          {
            "$ref": "#/components/schemas/FilterStatusChanged"
          },
          {
            "$ref": "#/components/schemas/FilterStatusChangedAgo"
          },
          {
            "$ref": "#/components/schemas/FilterTag"
          },
          {
            "$ref": "#/components/schemas/FilterText"
          },
          {
            "$ref": "#/components/schemas/FilterType"
          }
        ]
      },
      "FilterAdded": {
        "properties": {
          "end": {
            "description": "miliseconds since UNIX epoch",
            "type": "integer"
          },
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "start": {
            "description": "miliseconds since UNIX epoch",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterAddedAgo": {
        "properties": {
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "offset": {
            "description": "miliseconds since the current time",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterAnd": {
        "properties": {
          "filters": {
            "description": "the filters which must be all be satisified",
            "items": {
              "$ref": "#/components/schemas/Filter"
            },
            "type": "array"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterChecked": {
        "properties": {
          "end": {
            "description": "miliseconds since UNIX epoch",
            "type": "integer"
          },
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "start": {
            "description": "miliseconds since UNIX epoch",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterCheckedAgo": {
        "properties": {
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "offset": {
            "description": "miliseconds since the current time",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterExternal": {
        "properties": {
          "end": {
            "description": "miliseconds since UNIX epoch",
            "type": "integer"
          },
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "start": {
            "description": "miliseconds since UNIX epoch",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterExternalAgo": {
        "properties": {
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "offset": {
            "description": "miliseconds since the current time",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterId": {
        "properties": {
          "filters": {
            "description": "the action IDs; note that action IDs are unique, but even across restarts",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterOr": {
        "properties": {
          "filters": {
            "description": "the filters from which at least one must be satisified",
            "items": {
              "$ref": "#/components/schemas/Filter"
            },
            "type": "array"
          },
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterRegex": {
        "properties": {
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "pattern": {
            "description": "a Java regular expression the action must match",
            "type": "regex"
          }
        },
        "type": "object"
      },
      "FilterSourceFile": {
        "properties": {
          "files": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterSourceLocation": {
        "properties": {
          "locations": {
            "items": {
              "properties": {
                "column": {
                  "type": "integer"
                },
                "file": {
                  "type": "string"
                },
                "line": {
                  "type": "integer"
                },
                "time": {
                  "type": "integer"
                }
              },
              "type": "object"
            },
            "type": "array"
          },
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterStatus": {
        "properties": {
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "states": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterStatusChanged": {
        "properties": {
          "end": {
            "description": "miliseconds since UNIX epoch",
            "type": "integer"
          },
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "start": {
            "description": "miliseconds since UNIX epoch",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterStatusChangedAgo": {
        "properties": {
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "offset": {
            "description": "miliseconds since the current time",
            "type": "integer"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterTag": {
        "properties": {
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "tags": {
            "description": "tags which must appear attached to the action",
            "type": {
              "items": {
                "type": "string"
              },
              "type": "array"
            }
          }
        },
        "type": "object"
      },
      "FilterText": {
        "properties": {
          "matchCase": {
            "description": "whether the text must match as provided or if the case can be different",
            "type": "boolean"
          },
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "text": {
            "description": "text which must appear in the action",
            "type": "string"
          }
        },
        "type": "object"
      },
      "FilterType": {
        "properties": {
          "negate": {
            "description": "if true, the filter's output will be inverted",
            "type": "boolean"
          },
          "type": {
            "type": "string"
          },
          "types": {
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    }
  },
